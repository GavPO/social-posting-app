  "devDependencies": {
    "concurrently": "^7.5.0"
  },
  "scripts": {
    "start": "cd server && node server.js --ignore client",
    "develop": "concurrently \"cd server && npm run start\" \"cd client && npm start\"",
    "client": "cd client && npm run start",
